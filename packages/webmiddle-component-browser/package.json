{
  "name": "webmiddle-component-browser",
  "version": "0.5.1",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^6.14.0 || >=8.10.0"
  },
  "scripts": {
    "test": "ava",
    "test:watch": "yarn run test -- --watch",
    "clean": "rimraf dist",
    "babel": "babel src --out-dir dist --source-maps inline",
    "build": "yarn run clean && yarn run babel",
    "build:watch": "yarn run clean && yarn run babel -- --watch",
    "prepublish": "yarn run build"
  },
  "babel": {
    "extends": "../../babel.config.js"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "sources": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "extends": "../../babel.config.js"
      }
    }
  },
  "keywords": [
    "web",
    "data",
    "integration",
    "extraction",
    "scraper",
    "jsx",
    "framework"
  ],
  "author": {
    "name": "Manuel Dell'Elce",
    "url": "http://github.com/Maluen/"
  },
  "repository": "webmiddle/webmiddle",
  "license": "MIT",
  "peerDependencies": {
    "webmiddle": "^0.5.1"
  },
  "dependencies": {
    "puppeteer": "1.17.0"
  },
  "devDependencies": {
    "webmiddle": "^0.5.1"
  }
}
