{
  "name": "webmiddle-component-browser",
  "version": "0.4.0-alpha.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^6.9.0 || >=8.0.0"
  },
  "scripts": {
    "test": "ava",
    "test:watch": "yarn run test -- --watch",
    "clean": "rimraf dist",
    "babel": "babel src --out-dir dist --source-maps inline",
    "build": "yarn run clean && yarn run babel",
    "build:watch": "yarn run clean && yarn run babel -- --watch",
    "prepublish": "yarn run build"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "extends": "../../.babelrc"
    }
  },
  "keywords": [
    "web",
    "data",
    "integration",
    "extraction",
    "scraper",
    "jsx",
    "framework"
  ],
  "author": {
    "name": "Manuel Dell'Elce",
    "url": "http://github.com/Maluen/"
  },
  "repository": "webmiddle/webmiddle",
  "license": "MIT",
  "peerDependencies": {
    "webmiddle": "^0.3.0"
  },
  "dependencies": {
    "puppeteer": "^1.5.0"
  },
  "devDependencies": {
    "webmiddle": "^0.4.0-alpha.0"
  }
}
