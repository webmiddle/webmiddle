{
  "name": "webmiddle-service-arraymap",
  "version": "0.2.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^6.2.2 || >=8.0.0"
  },
  "scripts": {
    "test": "ava",
    "test:watch": "yarn run test -- --watch",
    "clean": "node ../../scripts/rimraf dist",
    "babel": "node ../../scripts/npmRun babel src --out-dir dist --source-maps inline",
    "build": "yarn run clean && yarn run babel",
    "build:watch": "yarn run clean && yarn run babel -- --watch",
    "prepublish": "yarn run build"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "extends": "../../.babelrc"
    }
  },
  "keywords": [
    "web",
    "data",
    "integration",
    "extraction",
    "scraper",
    "jsx",
    "framework"
  ],
  "author": {
    "name": "Manuel Dell'Elce",
    "url": "http://github.com/Maluen/"
  },
  "repository": "webmiddle/webmiddle",
  "license": "MIT",
  "dependencies": {
    "webmiddle": "^0.2.0"
  },
  "devDependencies": {
    "ava": "^0.22.0"
  }
}
