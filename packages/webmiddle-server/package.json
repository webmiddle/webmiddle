{
  "name": "webmiddle-server",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "ava",
    "test:watch": "npm run test -- --watch",
    "clean": "node ../../scripts/rimraf dist",
    "babel": "babel src --out-dir dist --source-maps inline",
    "build": "npm run clean && npm run babel",
    "build:watch": "npm run clean && npm run babel -- --watch",
    "prepublish": "npm run build"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "extends": "../../.babelrc"
    }
  },
  "keywords": [
    "web",
    "data",
    "integration",
    "extraction",
    "scraper",
    "jsx",
    "framework"
  ],
  "author": {
    "name": "Manuel Dell'Elce",
    "url": "http://github.com/Maluen/"
  },
  "repository": "webmiddle/webmiddle",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "lodash": "^4.17.4",
    "webmiddle": "^0.1.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1"
  }
}
